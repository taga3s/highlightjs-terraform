{
  "name": "@taga3s/highlightjs-terraform",
  "version": "1.0.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "highlight.js syntax definition for Ethereum's terraform language",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "biome format --write ./src",
    "lint": "biome lint --write --unsafe ./src",
    "check": "biome check --write src",
    "test": "vitest run"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/taga3s/highlightjs-terraform.git"
  },
  "keywords": [
    "terraform",
    "highlight.js",
    "highlightjs",
    "syntax"
  ],
  "author": "taga3s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taga3s/highlightjs-terraform/issues"
  },
  "homepage": "https://github.com/taga3s/highlightjs-terraform#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.21",
    "highlight.js": "^11.11.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
